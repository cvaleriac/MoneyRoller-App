
<h1>Your outgoing rollovers</h1>

<p>Amount: <%= @rollover.amount %></p>
<p>Origin bank: <%= @rollover.origin %></p>
<p>Destination: <%= @rollover.institution.name %></p>
 

<p><%= link_to "Edit rollover", edit_rollover_path(@rollover)  %></p>
<p><%= link_to "Delete rollover", rollover_path(@rollover), method: :delete, data: { confirm: "You sure?" } %></p>

<p><%= link_to "Back", rollovers_path(@rollover)%></p>